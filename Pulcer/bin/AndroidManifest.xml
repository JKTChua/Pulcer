<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pulcer"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="10" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.pulcer.Launcher"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.pulcer.Settings"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name="com.me.pulcer.MainActivity"
            android:label="@string/title_activity_main" >
        </activity>
        <activity
            android:name="com.example.pulcer.Login"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name="com.example.pulcer.Dashboard"
            android:label="@string/title_activity_dashboard" >
        </activity>
        <activity
            android:name="com.example.pulcer.Register"
            android:label="@string/title_activity_register" >
        </activity>
        <activity
            android:name="com.example.pulcer.Searchmed"
            android:label="@string/title_activity_searchmed"
            android:parentActivityName="com.example.searchmed" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.searchmed" />
        </activity>
    </application>

</manifest>